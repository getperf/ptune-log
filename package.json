{
  "name": "ptune-log",
  "version": "1.0.0",
  "description": "Obsidian plugin for worklog management with Google Tasks and AI-powered note analysis.",
  "main": "main.js",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "prepare-dist": "node scripts/prepare-dist.mjs",
    "zip": "node scripts/make-zip.mjs",
    "dist": "npm run build && npm run prepare-dist && npm run zip",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "Minoru Furusawa",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "cross-env": "^10.0.0",
    "cross-env-shell": "^7.0.3",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "obsidian": "^1.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "tslib": "2.4.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "obsidian-daily-notes-interface": "^0.9.4"
  }
